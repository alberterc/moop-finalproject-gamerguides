<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.moop.gamerguides" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GamerGuides" >
        <activity
            android:name=".userprofile.AccountSettings"
            android:exported="true" />
        <activity
            android:name=".EditCourse"
            android:exported="true" />
        <activity
            android:name=".getstarted.InitialSignUp"
            android:exported="true" />
        <activity
            android:name=".getstarted.GetStarted"
            android:exported="true" />
        <activity
            android:name=".login.SignUp"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login.SignIn"
            android:exported="true" />
        <activity
            android:name=".MainMenu"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>